{
    "name": "snowsoft/code-mirror",
    "description": "code editor extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/snowsoft/code-mirror",
    "license": "MIT",
    "authors": [
        {
            "name": "jxlwqq",
            "email": "jxlwqq@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "snowsoft/laravel-admin": ">=1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Jxlwqq\\CodeMirror\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jxlwqq\\CodeMirror\\CodeMirrorServiceProvider"
            ]

        }
    }
}
